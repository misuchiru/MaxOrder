!!! XML
%workbook{:xmlns => "urn:schemas-microsoft-com:office:spreadsheet", "xmlns:html" => "http://www.w3.org/TR/REC-html40", "xmlns:o" => "urn:schemas-microsoft-com:office:office", "xmlns:ss" => "urn:schemas-microsoft-com:office:spreadsheet", "xmlns:x" => "urn:schemas-microsoft-com:office:excel"}
  %worksheet{"ss:name" => "Sheet1"}
    %table
      %row
        %cell
          %data{"ss:type" => "String"} 序
        %cell
          %data{"ss:type" => "String"} 姓名
        %cell
          %data{"ss:type" => "String"} 群組
        %cell
          %data{"ss:type" => "String"} 職稱
        %cell
          %data{"ss:type" => "String"} 電話
        %cell
          %data{"ss:type" => "String"} 信箱
      - @contacts.each do |contact|
        %row
          %cell
            %data{"ss:type" => "Number"}= contact.id
          %cell
            %data{"ss:type" => "String"}= contact.name
          %cell
            %data{"ss:type" => "String"}= contact.groups
          %cell
            %data{"ss:type" => "String"}= contact.title
          %cell
            %data{"ss:type" => "String"}= contact.phone
          %cell
            %data{"ss:type" => "String"}= contact.email
